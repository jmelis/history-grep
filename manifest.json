{
  "manifest_version": 3,
  "name": "HistoryGrep",
  "version": "1.0.0",
  "description": "Enhanced search interface for Chrome's browsing history with fuzzy search and advanced filtering",
  "permissions": [
    "history",
    "tabs"
  ],
  "action": {
    "default_title": "HistoryGrep"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}